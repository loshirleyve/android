<?xml version="1.0" encoding="utf-8"?>
<repositorys>

    <!--获取控制编码-->
    <repository>
        <name>QueryCtrlCode</name>
        <action>com.yun9.sys.md.service.QueryMdCtrlcodeService</action>
        <output>
            <classname>com.yun9.wservice.model.CtrlCode</classname>
            <type>list</type>
        </output>
        <type>yun9</type>
    </repository>

    <!--获取程序更新-->
    <repository>
        <name>QuerySysprogramupdate</name>
        <action>com.yun9.sys.program.service.QuerySysprogramupdateService</action>
        <output>
            <classname>com.yun9.wservice.model.UpdateProgramBean</classname>
            <type>object</type>
        </output>
        <type>yun9</type>
    </repository>
</repositorys>