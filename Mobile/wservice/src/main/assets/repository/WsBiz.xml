<?xml version="1.0" encoding="utf-8"?>
<repositorys>
    <!-- 获取话题服务 -->
    <repository>
        <name>QueryTopics</name>
        <action>com.yun9.ws.biz.service.QueryTopicsService</action>
        <output>
            <classname>com.yun9.wservice.model.TopicBean</classname>
            <type>list</type>
        </output>
        <type>yun9</type>
    </repository>

    <!-- 获取服务分类根据城市 -->
    <repository>
        <name>QueryMdProductGroupBylocation</name>
        <action>com.yun9.ws.biz.service.QueryMdProductGroupBylocationService</action>
        <output>
            <classname>com.yun9.wservice.model.ProductGroup</classname>
            <type>list</type>
        </output>
        <type>yun9</type>
    </repository>

    <!-- 查询产品信息 -->
    <repository>
        <name>QueryProducts</name>
        <action>com.yun9.ws.biz.service.QueryProductsByGroupIdService</action>
        <output>
            <classname>com.yun9.wservice.model.Product</classname>
            <type>list</type>
        </output>
        <type>yun9</type>
    </repository>

    <!-- 查询产品信息通过产品id -->
    <repository>
        <name>QueryProductInfoById</name>
        <action>com.yun9.ws.biz.service.QueryProductInfoByIdService</action>
        <output>
            <classname>com.yun9.wservice.model.Product</classname>
            <type>object</type>
        </output>
        <type>yun9</type>
    </repository>

</repositorys>