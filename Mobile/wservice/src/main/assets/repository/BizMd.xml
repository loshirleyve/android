<?xml version="1.0" encoding="utf-8"?>
<repositorys>
    <!-- 获取支持的服务城市 -->
    <repository>
        <name>QueryCities</name>
        <action>com.yun9.ws.biz.service.QueryCitiesService</action>
        <output>
            <classname>com.yun9.wservice.model.ServiceCity</classname>
            <type>list</type>
        </output>
        <type>yun9</type>
    </repository>
    
    <!-- 初始化机构服务 -->
    <repository>
        <name>InstInit</name>
        <action>com.yun9.ws.biz.service.InstInitService</action>
        <type>yun9</type>
    </repository>

    <!-- 获取场景 -->
    <repository>
        <name>QuerySceneService</name>
        <action>com.yun9.ws.biz.service.QuerySceneService</action>
        <output>
            <classname>com.yun9.wservice.model.Scene</classname>
            <type>list</type>
        </output>
        <type>yun9</type>
    </repository>
    <!-- 查询控制编码服务 -->
    <repository>
        <name>QueryMdInstScale</name>
        <action>com.yun9.ws.biz.service.QueryMdInstScaleService</action>
        <output>
            <classname>com.yun9.wservice.model.MdInstScales</classname>
            <type>object</type>
        </output>
        <type>yun9</type>
    </repository>
</repositorys>