<?xml version="1.0" encoding="utf-8"?>
<repositorys>
    <!-- 查询机构客户信息 -->
    <repository>
        <name>QueryInstClients</name>
        <action>com.yun9.ws.biz.service.QueryInstClientsService</action>
        <output>
            <classname>com.yun9.wservice.model.Client</classname>
            <type>list</type>
        </output>
        <type>yun9</type>
    </repository>

    <!--保存机构客户信息-->
    <repository>
        <name>AddOrUpdateInstClientsService</name>
        <action>com.yun9.ws.biz.service.AddOrUpdateInstClientsService</action>
        <output>
            <type>object</type>
        </output>
        <type>yun9</type>
    </repository>

    <!-- 通过id查询机构客户信息 -->
    <repository>
        <name>QueryInstClientById</name>
        <action>com.yun9.ws.biz.service.QueryInstClientByIdService</action>
        <output>
            <classname>com.yun9.wservice.model.Client</classname>
            <type>object</type>
        </output>
        <type>yun9</type>
    </repository>

</repositorys>