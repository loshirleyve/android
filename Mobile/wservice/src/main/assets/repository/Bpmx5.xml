<?xml version="1.0" encoding="utf-8"?>
<repositorys>
	<repository>
		<name>BpmDoToDoService</name>
		<action>com.yun9.bpm.service.BpmDoToDoService</action>
		<output>      
			<classname>java.lang.Boolean</classname>
			<type>object</type>
		</output>
		<type>bpmx5</type>
	</repository>
	<!-- 根据流程实例ID获取流程节点列表 -->
	<repository>
		<name>BpmMsgCardRefService</name>
		<action>com.yun9.bpm.service.BpmMsgCardRefService</action>
		<output>      
			<classname>com.yun9.mobile.msg.model.BpmNodeInfo</classname>
			<type>list</type>
		</output>
		<type>bpmx5</type>
	</repository>
	<!-- 查询流程节点信息 -->
	<repository>
		<name>BpmNodeQueryService</name>
		<action>com.yun9.bpm.service.BpmNodeQueryService</action>
		<output>      
			<classname>com.yun9.mobile.bpm.model.ProcessConfigInfo</classname>
			<type>object</type>
		</output>
		<type>bpmx5</type>
	</repository>
	<!-- 启动流程 -->
	<repository>
		<name>BpmStartService</name>
		<action>com.yun9.bpm.service.BpmStartService</action>
		<output> 
			<classname>java.lang.Boolean</classname>
			<type>object</type>
		</output>
		<type>bpmx5</type>
	</repository>
		<!-- 查询流程定义-->
	<repository>
		<name>BpmDefQuery4AppService</name>
		<action>com.yun9.bpm.service.BpmDefQuery4AppService</action>
		<output> 
			<classname>com.yun9.mobile.bpm.model.ProcessDefInfo</classname>
			<type>object</type>
		</output>
		<type>bpmx5</type>
	</repository>
</repositorys>