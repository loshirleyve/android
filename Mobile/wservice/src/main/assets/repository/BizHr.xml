<?xml version="1.0" encoding="utf-8"?>
<repositorys>
    <!-- 获取用户排班信息 -->
	<repository>
		<name>BizHrQueryUserSchedulingService</name>
		<action>com.yun9.biz.hr.service.BizHrQueryUserSchedulingService</action>
		<output>
			<classname>com.yun9.mobile.framework.model.server.hr.ModelQueryUserScheduleInfo</classname>
			<type>list</type>
		</output>
		<type>yun9</type>
	</repository>
	
	<!-- 获取机构班次信息 -->
	<repository>
		<name>BizHrFindInstShiftService</name>
		<action>com.yun9.biz.hr.service.BizHrFindInstShiftService</action>
		<output>    
			<classname>com.yun9.mobile.framework.model.server.hr.ModelFindInstShiftInfo</classname>
			<type>list</type>
		</output>
		<type>yun9</type>
	</repository>
	
	<!-- 新增/修改用户排班信息 -->
	<repository>
		<name>BizHrSchedulingSaveService</name>
		<action>com.yun9.biz.hr.service.BizHrSchedulingSaveService</action>
		<output>
			<classname>com.yun9.mobile.framework.model.server.hr.ModelSaveUserScheduleInfo</classname>
			<type>object</type>
		</output>
		<type>yun9</type>
	</repository>
	
	<!-- 删除用户排班信息 -->
	<repository>
		<name>BizHrSchedulingRemoveService</name>
		<action>com.yun9.biz.hr.service.BizHrSchedulingRemoveService</action>
		<output>
			<classname>com.yun9.mobile.framework.model.server.hr.ModelRemoveUserScheduleInfo</classname>
			<type>object</type>
		</output>
		<type>yun9</type>
	</repository>
	    
</repositorys>