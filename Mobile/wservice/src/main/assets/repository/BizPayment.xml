<?xml version="1.0" encoding="utf-8"?>
<repositorys>
    <!-- 查询充值类型列表服务 -->
    <repository>
        <name>QueryRechargeTypeService</name>
        <action>com.yun9.ws.biz.service.QueryRechargeTypeService</action>
        <output>
            <classname>com.yun9.wservice.model.RechargeType</classname>
            <type>list</type>
        </output>
        <type>yun9</type>
    </repository>

    <!-- 登记充值服务 -->
    <repository>
        <name>AddRechargeService</name>
        <action>com.yun9.ws.biz.service.AddRechargeService</action>
        <output>
            <classname>com.yun9.wservice.model.AddRechargeResult</classname>
            <type>object</type>
        </output>
        <type>yun9</type>
    </repository>

    <!-- 根据充值状态查询充值记录列表服务 -->
    <repository>
        <name>QueryRechargeService</name>
        <action>com.yun9.ws.biz.service.QueryRechargeService</action>
        <output>
            <classname>com.yun9.wservice.model.RechargeRecord</classname>
            <type>list</type>
        </output>
        <type>yun9</type>
    </repository>

    <!-- 登记充值服务 -->
    <repository>
        <name>QueryCollectsBySourceService</name>
        <action>com.yun9.ws.biz.service.QueryCollectsBySourceService</action>
        <output>
            <classname>com.yun9.wservice.model.FinanceCollects</classname>
            <type>object</type>
        </output>
        <type>yun9</type>
    </repository>

    <!-- 提交支付凭证 -->
    <repository>
        <name>UpdateCollectByArriveInfoService</name>
        <action>com.yun9.ws.biz.service.UpdateCollectByArriveInfoService</action>
        <type>yun9</type>
    </repository>

</repositorys>