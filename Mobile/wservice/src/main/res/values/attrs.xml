<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MsgCardWidget">
        <attr name="showLocation" format="boolean"></attr>
        <attr name="showAttachment" format="boolean" />
        <attr name="showToolbar" format="boolean" />
        <attr name="showLastComment" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ShowCommentWidget">
        <attr name="isIndicator" format="boolean"></attr>
        <attr name="headTitle" format="string"></attr>
        <attr name="time" format="string"></attr>
        <attr name="content" format="string"></attr>
        <attr name="rating" format="float"></attr>
        <attr name="numsOfStar" format="integer"></attr>
    </declare-styleable>
</resources>
