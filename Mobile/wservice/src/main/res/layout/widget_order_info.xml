<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <com.yun9.wservice.view.order.OrderProviderWidget
        android:id="@+id/inst_cell"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <View
        android:id="@+id/inst_line"
        android:layout_below="@id/inst_cell"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/devide_line"/>
    <com.yun9.jupiter.widget.JupiterListView
        android:id="@+id/product_list"
        android:layout_below="@id/inst_line"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <View
        android:layout_below="@id/product_list"
        android:layout_width="match_parent"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:layout_height="0.5dp"
        android:background="@color/devide_line"/>
</RelativeLayout>