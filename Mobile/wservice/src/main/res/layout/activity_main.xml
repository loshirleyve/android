<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.yun9.wservice.layout.TitleRelativeLayout
        android:id="@+id/main_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <FrameLayout
        android:id="@+id/main_fl_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/main_line"
        android:layout_below="@+id/main_title"/>

    <View
        android:id="@+id/main_line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/devide_line"
        android:layout_above="@+id/button_bar_main_group"/>


    <include android:id="@+id/button_bar_main_group"  layout="@layout/button_bar_main"></include>


</RelativeLayout>
