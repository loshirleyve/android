<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/popup_window_backround">

    <com.yun9.jupiter.widget.JupiterSpringbackScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <com.yun9.jupiter.view.JupiterGridView
                android:id="@+id/city_gv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:horizontalSpacing="5dp"
                android:numColumns="3"
                android:padding="10dp"
                android:scrollbars="none"
                android:verticalSpacing="5dp"></com.yun9.jupiter.view.JupiterGridView>

            <com.yun9.jupiter.view.JupiterGridView
                android:id="@+id/district_gv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:horizontalSpacing="5dp"
                android:numColumns="3"
                android:padding="10dp"
                android:scrollbars="none"
                android:verticalSpacing="5dp">

            </com.yun9.jupiter.view.JupiterGridView>
        </LinearLayout>


    </com.yun9.jupiter.widget.JupiterSpringbackScrollView>

</RelativeLayout>